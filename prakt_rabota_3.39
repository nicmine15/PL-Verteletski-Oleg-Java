import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		String text = scanner.nextLine();
		String[] razdeltext = text.split(" ");
		String bukva = "";
		String slovo = "";
		for (int i = 0; i < razdeltext.length; i++) {
		    bukva += razdeltext[i].substring(0, 1);
		    slovo += razdeltext[i].substring(1, razdeltext[i].length()) + " ";
		}
		System.out.print(slovo + bukva + " ауч.");
	}
}
